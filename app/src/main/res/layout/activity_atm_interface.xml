<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".AtmInterfaceActivity">

    <ImageView
        android:id="@+id/logoAtm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:src="@drawable/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btnCliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:src="@drawable/menu_cliente"
        app:layout_constraintStart_toStartOf="@id/logoAtm"
        app:layout_constraintTop_toBottomOf="@id/logoAtm" />

    <ImageView
        android:id="@+id/btnEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:src="@drawable/menu_empresa"
        app:layout_constraintEnd_toEndOf="@id/logoAtm"
        app:layout_constraintTop_toBottomOf="@id/logoAtm" />

    <ImageView
        android:id="@+id/btnServico"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:src="@drawable/menu_servico"
        app:layout_constraintStart_toStartOf="@id/logoAtm"
        app:layout_constraintTop_toBottomOf="@id/btnCliente" />

    <ImageView
        android:id="@+id/btnContato"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:src="@drawable/menu_contato"
        app:layout_constraintEnd_toEndOf="@id/logoAtm"
        app:layout_constraintTop_toBottomOf="@id/btnEmpresa" />


</androidx.constraintlayout.widget.ConstraintLayout>